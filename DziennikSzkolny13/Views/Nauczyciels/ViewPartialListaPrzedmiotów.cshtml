            @Html.DisplayFor(modelItem => item.przedmiotNauczyciel.Imie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NazwaPrzedmiotu)
        </td>
        <td>
        </td>
    </tr>
}
</table>
