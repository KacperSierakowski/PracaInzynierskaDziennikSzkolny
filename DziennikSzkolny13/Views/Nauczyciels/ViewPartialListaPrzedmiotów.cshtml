            @Html.DisplayNameFor(model => model.przedmiotNauczyciel.Imie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NazwaPrzedmiotu)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
